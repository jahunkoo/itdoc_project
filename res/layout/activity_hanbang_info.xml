<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:orientation="horizontal"
            android:weightSum="5" >

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:background="@drawable/rounded_corner_box" >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:text="아이" />
            </RelativeLayout>

            <TextView
                android:id="@+id/textview_hanbang_info_main_title"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:gravity="center"
                android:text="@string/textview_hanbang_info_up_main_title"
                android:textAppearance="?android:attr/textAppearanceLarge" />

            <TextView
                android:id="@+id/textview_hanbang_info_main_time"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|left"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/textview_hanbang_info_up_main_time"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textSize="10sp" />
        </LinearLayout>

        <TextView style="@style/suppoters_line_text" />

        <TextView
            android:id="@+id/textview_hanbang_info_main1"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            style="@style/suppoters_sentence"
            android:text="@string/textview_hanbang_info_up_main1" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:scaleType="centerCrop"
            android:src="@drawable/theme3" />

        <TextView
            android:id="@+id/textview_hanbang_info_0"
            style="@style/suppoters_sentence_bold"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:text="@string/textview_hanbang_info_up_main2_title" />

        <TextView
            style="@style/suppoters_sentence"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:text="@string/textview_hanbang_info_up_main2" />

        <TextView
            android:id="@+id/textview_hanbang_info_1"
            style="@style/suppoters_sentence_bold"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:text="@string/textview_hanbang_info_up_main3_title" />

        <TextView
            style="@style/suppoters_sentence"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:text="@string/textview_hanbang_info_up_main3" />

        <TextView
            style="@style/suppoters_sentence"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:text="@string/textview_hanbang_info_up_main3_2" />

        <TextView
            android:id="@+id/textview_hanbang_info_2"
            style="@style/suppoters_sentence_bold"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:text="@string/textview_hanbang_info_up_main3_3_0" />

        <TextView
            style="@style/suppoters_sentence"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:text="@string/textview_hanbang_info_up_main3_3" />

        <TextView
            style="@style/suppoters_sentence"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:text="@string/textview_hanbang_info_up_main3_4" />

        <TextView
            style="@style/suppoters_sentence"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:text="@string/textview_hanbang_info_up_main3_5" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:scaleType="centerCrop"
            android:src="@drawable/hanbang_info_up_mother_baby" />

        <TextView
            android:id="@+id/textview_hanbang_info_3"
            style="@style/suppoters_sentence_bold"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:text="@string/textview_hanbang_info_up_main4_title" />

        <TextView
            style="@style/suppoters_sentence"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:text="@string/textview_hanbang_info_up_main4" />

        <TextView
            style="@style/suppoters_sentence"
            android:layout_marginBottom="-15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:text="@string/textview_hanbang_info_up_main4_2" />

        <TextView
            style="@style/suppoters_new_line_text"
            android:layout_marginBottom="15dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
             />
        
        <TextView
            style="@style/suppoters_basic_text"
            android:layout_gravity="right"
            android:layout_marginBottom="15dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            
            android:lineSpacingExtra="0dp"
            android:text="청구경희한의원으로 가시려면 아래를 click"
            android:textColor="@android:color/darker_gray" />


        <LinearLayout
            android:id="@+id/content_frame"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="10dp"
            android:orientation="horizontal" >
        </LinearLayout>
    </LinearLayout>

</ScrollView>